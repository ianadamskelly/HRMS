
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read access to company logos
    match /company/logo/{allPaths=**} {
      allow read;
    }

    // Restrict writes to authenticated users only
    match /{allPaths=**} {
      allow write: if request.auth != null;
    }
  }
}
